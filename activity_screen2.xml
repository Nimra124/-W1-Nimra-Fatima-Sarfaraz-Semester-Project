<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Screen2">

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">


        <ImageButton
            android:id="@+id/menubutton"
            android:layout_width="34dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="#009688"
            android:tint="#100F0F"
            app:srcCompat="@android:drawable/ic_menu_sort_by_size" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="129dp"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:layout_weight="1"
            android:background="#009688"
            android:text="   Furniture"


            android:textSize="30dp" />

        <ImageButton
            android:id="@+id/imageButton2"
            android:layout_width="34dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="#009688"
            android:tint="#121111"
            app:srcCompat="@android:drawable/ic_menu_search" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="match_parent"
        android:layout_height="46dp"
        android:layout_weight="1"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.41"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout"
        app:layout_constraintVertical_bias="0.0">


        <EditText
            android:id="@+id/editTextTextPersonName6"
            android:layout_width="312dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="Search for product here" />


        <ImageButton
            android:id="@+id/imageButton3"
            android:layout_width="34dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="#F2F6F6"
            android:tint="#121111"
            app:srcCompat="@android:drawable/ic_menu_search" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="match_parent"
        android:layout_height="39dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout2"
        app:layout_constraintVertical_bias="0.0">


        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:layout_weight="1"
            android:text="      Price"
            android:textSize="25dp"

            />

        <EditText
            android:id="@+id/min"
            android:layout_width="131dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="min" />

        <EditText
            android:id="@+id/max"
            android:layout_width="127dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="max" />
    </LinearLayout>


    <Button
        android:id="@+id/buttton_c"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#009688"
        android:text="Categories                                          See All  >"
        app:backgroundTint="#009688"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.453"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout3"
        app:layout_constraintVertical_bias="0.0" />

    <ImageButton
        android:id="@+id/imageButton4"
        android:layout_width="388dp"
        android:layout_height="90dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/download__4_"
        tools:layout_editor_absoluteY="200dp" />

    <ImageButton
        android:id="@+id/imageButton5"
        android:layout_width="388dp"
        android:layout_height="90dp"
        android:layout_marginStart="4dp"
        android:layout_marginLeft="4dp"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/download__3_"
        tools:layout_editor_absoluteY="328dp" />

    <ImageButton
        android:id="@+id/imageButton8"
        android:layout_width="388dp"
        android:layout_height="90dp"
        android:layout_marginStart="4dp"
        android:layout_marginLeft="4dp"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/childfur"
        tools:layout_editor_absoluteY="568dp" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="331dp"
        android:layout_height="30dp"
        android:background="#009688"
        android:text="Children Furniture"
        tools:layout_editor_absoluteX="49dp"
        tools:layout_editor_absoluteY="658dp" />

    <ImageButton
        android:id="@+id/imageButton7"
        android:layout_width="388dp"
        android:layout_height="90dp"
        android:layout_marginStart="4dp"
        android:layout_marginLeft="4dp"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/download__5_"
        tools:layout_editor_absoluteY="448dp" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="331dp"
        android:layout_height="30dp"
        android:background="#009688"
        android:text="Bed Room Furniture"
        tools:layout_editor_absoluteX="49dp"
        tools:layout_editor_absoluteY="298dp" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="331dp"
        android:layout_height="30dp"
        android:background="#009688"
        android:text="Living room furniture"
        tools:layout_editor_absoluteX="49dp"
        tools:layout_editor_absoluteY="418dp" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="331dp"
        android:layout_height="30dp"
        android:background="#009688"
        android:text="Dining Room Furniture"
        tools:layout_editor_absoluteX="49dp"
        tools:layout_editor_absoluteY="538dp" />

    <ImageButton
        android:id="@+id/imageButton6"
        android:layout_width="388dp"
        android:layout_height="90dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/download__4_"
        tools:layout_editor_absoluteY="200dp" />


</androidx.constraintlayout.widget.ConstraintLayout>